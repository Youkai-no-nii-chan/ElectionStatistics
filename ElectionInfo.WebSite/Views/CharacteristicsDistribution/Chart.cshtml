@model CharacteristicsDistributionRequest

@{
    var routeData = new RouteValueDictionary
    {
        { "ElectionId", Model.ElectionId },
        { "DistrictId", Model.DistrictId },
        { "DistributionValue", Model.DistributionValue },
        { "DistributionStepSize", Model.DistributionStepSize },
        { "ChartWidth", Model.ChartWidth },
        { "ChartHeight", Model.ChartHeight },
    };
    for (int i = 0; i < Model.DistributionParameters.Length; i++)
    {
        routeData.Add(string.Format("DistributionParameters[{0}]", i), Model.DistributionParameters[i]);
    }
}
<img src="@Url.Action("ChartImage", routeData)"/>